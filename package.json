{
  "name": "fastify-template",
  "version": "1.0.0",
  "description": "fastify template",
  "main": "index.js",
  "scripts": {
    "build": "cd ../client && yarn install && quasar build",
    "build-client-dev": "cd ../client && cp .env-local-server-dev .env && yarn install && quasar build -d && cp .env-dev .env",
    "build-client-uat": "cd ../client && cp .env-local-server-uat .env && yarn install && quasar build -d && cp .env-dev .env",
    "start:dev": "eval \"$(./set-env-dev.sh)\" npx nodemon src/index.js",
    "start:uat": "eval \"$(./set-env-uat.sh)\" npx nodemon src/index.js",
    "build-client:start:dev": "npm run build-client-dev && npm run start:dev",
    "build-client:start:uat": "npm run build-client-uat && npm run start:uat",
    "start": "node src/index.js",
    "start:all": "npm install && ./start-all.sh",
    "down:all": "./down-all.sh",
    "build:start": "npm run build && npm run start",
    "test": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^7.0.0",
    "@fastify/swagger": "^6.0.1",
    "@polkadot/util": "^9.3.1",
    "axios": "^0.27.2",
    "binary-uuid": "^2.0.3",
    "config": "^3.3.7",
    "date-diff": "^1.0.2",
    "fastify": "^3.29.0",
    "fastify-auth0-verify": "^0.7.4",
    "fastify-plugin": "^3.0.1",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "hjson": "^3.2.2",
    "http-errors": "^2.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "pino-pretty": "^7.6.1",
    "url-join": "^5.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@polkadot/keyring": "^9.7.2",
    "@polkadot/util-crypto": "^9.7.2",
    "@types/jest": "^27.5.1",
    "eslint": "^8.16.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "hasura-cli": "^2.7.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16"
  }
}
